{
  "openapi": "3.0.1",
  "info": {
    "title": "MapApi",
    "version": "1.0"
  },
  "paths": {
    "/api/Map/SetMap": {
      "post": {
        "tags": [
          "Map"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/Graph"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/Graph"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/Graph"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/Map/GetMap": {
      "get": {
        "tags": [
          "Map"
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/Map/ShortestRoute": {
      "get": {
        "tags": [
          "Map"
        ],
        "parameters": [
          {
            "name": "from",
            "in": "query",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "to",
            "in": "query",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/Map/ShortestDistance": {
      "get": {
        "tags": [
          "Map"
        ],
        "parameters": [
          {
            "name": "from",
            "in": "query",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "to",
            "in": "query",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "Edge": {
        "type": "object",
        "properties": {
          "fromId": {
            "type": "string",
            "nullable": true
          },
          "toId": {
            "type": "string",
            "nullable": true
          },
          "weight": {
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      },
      "Graph": {
        "type": "object",
        "properties": {
          "nodes": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Node"
            },
            "nullable": true
          },
          "edges": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Edge"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "Node": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "nullable": true
          },
          "x": {
            "type": "integer",
            "format": "int32"
          },
          "y": {
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      }
    }
  }
}